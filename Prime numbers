print("The prime numbers in the range from 1 to 100:")
a=1
b=". "+"2"
c=str(a)+str(b)
print(c)
mylist=[]
i=3
while i<=100:
    for j in range(2,i):
        if i%j == 0:
            break
    else:
        mylist.append(i)
    i=i+1
for k in range(1,len(mylist)+1):
    m=". "+ str(mylist[k-1])
    n= str(k+1)+str(m)
    print(n)
p=1+len(mylist)
p1=str(p)
print("There are ",p1," prime numbers.")
